#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/xbohodx01/work/theme_test/my-blog/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.28.0_async-validator@4.2.5_postcss@8.5.6_sass@_a3dd0f1c118bcb1950d275aaf61f1ecb/node_modules/vitepress/bin/node_modules:/home/xbohodx01/work/theme_test/my-blog/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.28.0_async-validator@4.2.5_postcss@8.5.6_sass@_a3dd0f1c118bcb1950d275aaf61f1ecb/node_modules/vitepress/node_modules:/home/xbohodx01/work/theme_test/my-blog/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.28.0_async-validator@4.2.5_postcss@8.5.6_sass@_a3dd0f1c118bcb1950d275aaf61f1ecb/node_modules:/home/xbohodx01/work/theme_test/my-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/xbohodx01/work/theme_test/my-blog/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.28.0_async-validator@4.2.5_postcss@8.5.6_sass@_a3dd0f1c118bcb1950d275aaf61f1ecb/node_modules/vitepress/bin/node_modules:/home/xbohodx01/work/theme_test/my-blog/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.28.0_async-validator@4.2.5_postcss@8.5.6_sass@_a3dd0f1c118bcb1950d275aaf61f1ecb/node_modules/vitepress/node_modules:/home/xbohodx01/work/theme_test/my-blog/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.28.0_async-validator@4.2.5_postcss@8.5.6_sass@_a3dd0f1c118bcb1950d275aaf61f1ecb/node_modules:/home/xbohodx01/work/theme_test/my-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
